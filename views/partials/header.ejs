    <meta
      name="keywords"
      content="KiSmovie, Xem phim mới Vietsub FullHD, Phim Online FullHD 2K 4K 8K, Phim Mới 2021 Hay Nhất, Thuyết minh tiếng Việt đầy đủ, Phim lẻ phim bộ 2021 FullHD Vietsub"
    />
    <meta expr:content="data:blog.pageTitle" property="og:title" />
    <meta expr:content="data:blog.url" property="og:url" />
    <meta content="KisMovies" property="og:site_name" />
    <meta content="629688671039918" property="fb:app_id" />
    <meta content="article" property="og:type" />
    <% if (movie) { %>
    <title>KiSmovies | <%- movie.title %></title>
    <meta name="keywords" content="<%- movie.slug %>" />
    <meta name="description"
    content="<%-movie.description.replace(/<\/?[^>]+(>|$)/g,
    "").trim().substr(0,160)+'...'%>" />
    <meta property="og:url" content="/phim/<%- movie.slug %>/xem-phim" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="KisMovie | <%- movie.title %>" />
    <meta property="og:description" content="<%-
    movie.description.replace(/<\/?[^>]+(>|$)/g,
    "").trim().substr(0,160)+'...'%>" />
    <meta property="og:image" content="<%- movie.movieThumb.full%>" />
    <meta property="og:image:alt" content="<%- movie.title %>" />
    <% } else { %>
    <title><%-title%></title>
    <meta
      name="keywords"
      content="KiSmovie, Xem phim mới Vietsub FullHD, Phim Online FullHD 2K 4K 8K, Phim Mới 2021 Hay Nhất, Thuyết minh tiếng Việt đầy đủ, Phim lẻ phim bộ 2021 FullHD Vietsub"
    />
    <meta
      property="og:title"
      content="Kismovie| Chia Sẻ Cảm Xúc Trong Mỗi Phút Phim Cùng Gia Đình!"
    />
    <meta
      property="og:description"
      content="Kismovie – Dịch Vụ Xem Phim Online 2021 miễn phí đem lại cho bạn cảm xúc như ở rạp và bên gia đình. Hơn nữa, đa dạng trên nhiều nền tảng là tự hào của chúng tôi"
    />
    <meta
      property="og:url"
      content="https://kismovie.com/"
      content="Kismovie| Chia Sẻ Cảm Xúc Trong Mỗi Phút Phim Cùng Gia Đình!"
    />
    <meta
      property="og:image"
      content="https://www.themoviedb.org/t/p/original/8Y43POKjjKDGI9MH89NW0NAzzp8.jpg"
    />
    <meta property="og:type" content="website" />
    <meta property="og:image:alt" content="KisMovie-Banner" />
    <% } %>
  </head>
</html>